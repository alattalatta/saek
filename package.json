    "@parcel/transformer-glsl": "2.5.0",
    "@swc/core": "^1.2.182",
    "@types/node": "16",
    "ava": "^4.2.0",
    "eslint": "^8.15.0",
    "eslint-plugin-lit": "^1.6.1",
    "eslint-plugin-wc": "^1.3.2",
    "parcel": "^2.5.0",
    "prettier": "^2.6.2",
    "ts-lit-plugin": "^1.2.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
